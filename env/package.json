{
  "name": "@harrypoggers/env",
  "version": "1.0.1",
  "description": "Environment utility for node js projects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "link": "tsup src/index.ts --format esm,cjs --dts --clean && npm link",
    "pre": "npm pack --dry-run"
  },
  "files": [
    "dist"
  ],
  "author": "harrypoggers",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
